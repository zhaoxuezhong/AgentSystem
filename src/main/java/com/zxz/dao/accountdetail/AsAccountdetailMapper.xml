<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxz.dao.accountdetail.AsAccountdetailMapper">

	<select id="getAsAccountdetailCount" resultType="int" parameterType="int">
		select count(1) from as_accountdetail where userId=#{userId}
	</select>

	<select id="findAsAccountdetailList" resultType="AsAccountdetail">
		select *from as_accountdetail where userId=#{userId} limit #{pageIndex},#{pageSize}
	</select>
	

</mapper>
